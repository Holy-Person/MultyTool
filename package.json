{
  "name": "MultyTool",
  "version": "0.4.0",
  "description": "MultyTool but now on electron-builder with yarn.",
  "license": "MIT",
  "scripts": {
    "dev": "electron-webpack dev",
    "compile": "electron-webpack",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "author": {
    "name": "Holy-Person",
    "email": "holyochi@gmail.com"
  },
  "build": {
    "appId": "com.electron.toolbyte2",
    "files": [
      "dist/electron/**/*",
      "node_modules/",
      "package.json"
    ],
    "win": {
      "icon": "build/icons/icon.ico",
      "target": [
        {
          "target": "nsis",
          "arch": [
            "ia32"
          ]
        }
      ]
    }
  },
  "dependencies": {
    "mousetrap": "^1.6.5",
    "source-map-support": "^0.5.19",
    "tone": "^14.7.58"
  },
  "devDependencies": {
    "electron": "^8.2.0",
    "electron-builder": "^22.4.1",
    "electron-webpack": "^2.8.2",
    "webpack": "~4.42.1"
  }
}
